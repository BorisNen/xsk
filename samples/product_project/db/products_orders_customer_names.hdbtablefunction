FUNCTION "Products_Orders_Customer_Names" ( )
   RETURNS Table (
       "Id" NVARCHAR(50),
       "CustomerName" NVARCHAR(500)
   )
   LANGUAGE SQLSCRIPT
   SQL SECURITY INVOKER AS
BEGIN
 
    RETURN
   
        SELECT      "Id",
                    "CustomerName"
        FROM "product_project.db::Products.Orders";
 
END;
